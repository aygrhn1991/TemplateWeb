
@{
    Layout = null;
}

@Styles.Render("~/bundles/css-lib")
@Scripts.Render("~/bundles/script-lib")

<script src="~/Plugin/layer/layer.js"></script>

<link href="~/Plugin/summernote/css/summernote.css" rel="stylesheet" />
<script src="~/Plugin/summernote/js/summernote.js"></script>
<script src="~/Plugin/summernote/js/lang/summernote-zh-CN.js"></script>

<link href="~/Content/admin/admin-public.css" rel="stylesheet" />
<script src="~/Scripts/admin/admin-public.js"></script>
<link href="~/Content/admin/admin-content.css" rel="stylesheet" />
<script src="~/Scripts/admin/admin-nav.js"></script>

<div class="tp-content" ng-app="app" ng-controller="navAdd">
    <form>
        <div class="form-group">
            <label>标题：</label>
            <input type="text" class="form-control" placeholder="请输标题..." ng-model="pageModel.title">
        </div>
        <div class="form-group text-center">
            <button type="button" class="btn btn-success" ng-click="Save()">保&nbsp;存</button>
        </div>
    </form>
</div>

